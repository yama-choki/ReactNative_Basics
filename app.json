    "web": {
      "favicon": "./assets/favicon.png"
    },
    "devDependencies": {
      "@babel/core": "^7.12.9",
      "@types/react": "~17.0.21",
      "@types/react-native": "~0.64.12",
      "typescript": "~4.3.5"
    },
    "resolutions": {
      "@types/react": "17.0.2",
      "@types/react-dom": "17.0.2"
    },
    "private": true
  }
